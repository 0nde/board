<% if(card != null){ %>
<a href="#/card/<%- card.attributes.id%>" class="highlight-icon pull-left"><%-card.attributes.name%></a><div class="pull-right"><a title="<%- i18next.t('Send to board') %>" class="js-send-card-to-board <% if (!_.isEmpty(role_links.where({slug: 'add_organization'}))) {%>send_back_to_archived_card <%}%>" data-card_id="<%-card.attributes.id%>"><i class="icon-reply"></i></a> <a class="js-delete-archived-card" data-card_id="<%-card.attributes.id%>" title="<%- i18next.t('Delete') %>"><i class="icon-remove btn btn-link"></i></a></div>
<% }else{ %>
<span class="alert alert-info">
	<%- i18next.t('No %s available.', { postProcess: 'sprintf', sprintf: [i18next.t('cards')] }) %>
</span>
<%}%>
